{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "node",
		"target": "ES2022",
		"outDir": "./dist",
		"rootDir": "./",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"ts-node": {
		"esm": true,
		"experimentalSpecifierResolution": "node",
		"swc": true
	},
	"include": ["server/**/*", "src/types/**/*"]
}
